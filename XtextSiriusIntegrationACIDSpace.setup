      xsi:type="setup:CompoundTask"
      disabled="true"
      name="Wiki">
    <setupTask
        xsi:type="git:GitCloneTask"
        id="wiki.git.clone"
        remoteURI="${gitlab.acidspace.nl.project}"
        checkoutBranch="master">
      <annotation
          source="http://www.eclipse.org/oomph/setup/InducedChoices">
        <detail
            key="inherit">
          <value>gitlab.acidspace.nl.wiki.remoteURIs</value>
        </detail>
        <detail
            key="label">
          <value>${scope.project.label} Wiki Git repository</value>
        </detail>
        <detail
            key="target">
          <value>remoteURI</value>
        </detail>
      </annotation>
      <configSections
          name="user">
        <properties
            key="name"
            value="${gitlab.acidspace.nl.user.name}"/>
        <properties
            key="email"
            value="${gitlab.acidspace.nl.user.email}"/>
      </configSections>
      <description>${scope.project.label}</description>
    </setupTask>
    <setupTask
